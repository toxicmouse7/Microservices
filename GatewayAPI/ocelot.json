{
  "Routes": [
    {
      "DownstreamHostAndPorts": [
        {
          "Host": "coffeeapi",
          "Port": 80
        }
      ],
      "DownstreamPathTemplate": "/{everything}",
      "UpstreamPathTemplate": "/coffee/{everything}",
      "UpstreamHttpMethod": [
        "Get"
      ],
      "RouteIsCaseSensitive": true,
      "DownstreamScheme": "http"
    },
    {
      "DownstreamHostAndPorts": [
        {
          "Host": "authapi",
          "Port": 80
        }
      ],
      "DownstreamPathTemplate": "/{everything}",
      "UpstreamPathTemplate": "/auth/{everything}",
      "UpstreamHttpMethod": [
        "Get"
      ],
      "RouteIsCaseSensitive": true,
      "DownstreamScheme": "http"
    }
  ]
}